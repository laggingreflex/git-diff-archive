{
  "name": "git-diff-archive",
  "version": "1.2.0",
  "description": "Archive of the diff files using node.js and git.",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run mocha:watch",
    "mocha": "mocha -t 4000",
    "mocha:watch": "mocha -t 4000 -w",
    "lint": "eslint index.js bin/git_diff_archive bin/gda test/**.js"
  },
  "keywords": [
    "git",
    "cli",
    "archive",
    "diff",
    "zip",
    "tar"
  ],
  "preferGlobal": true,
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tsuyoshiwada/git-diff-archive.git"
  },
  "bugs": {
    "url": "https://github.com/tsuyoshiwada/git-diff-archive/issues"
  },
  "homepage": "https://github.com/tsuyoshiwada/git-diff-archive",
  "author": "tsuyoshiwada",
  "license": "MIT",
  "bin": {
    "git_diff_archive": "bin/git_diff_archive",
    "gda": "bin/gda"
  },
  "dependencies": {
    "archiver": "^2.1.1",
    "cli-spinner": "^0.2.5",
    "cli-table": "^0.3.1",
    "colors": "^1.1.2",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "node-alias": "^1.0.4",
    "numeral": "^2.0.6",
    "object-assign": "^4.1.0",
    "pretty-bytes": "^4.0.2",
    "string-template": "^1.0.0",
    "update-notifier": "^2.5.0",
    "which": "^1.2.10"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "glob": "^7.0.3",
    "mocha": "^5.1.1",
    "power-assert": "^1.4.1",
    "rimraf": "^2.5.2",
    "unzip": "^0.1.11"
  }
}
